	eprint = {http://geology.gsapubs.org/content/early/2013/09/06/G34718.1.full.pdf+html},
	journal = {Geology},
	title = {Multidecadal rainfall variability in South Pacific Convergence Zone as revealed by stalagmite geochemistry},
